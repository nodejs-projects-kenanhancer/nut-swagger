{
  "name": "nut-swagger",
  "version": "1.0.9",
  "description": "It generates Controller JavaScript files from you swagger files",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-generate-swagger-empty-controller": "node index.js generate-default-controller ./swagger-definitions/greeting-definition.yaml ./services/controllers --overwrite true --isEmptyBody true",
    "test-generate-swagger-default-controller": "node index.js generate-default-controller ./swagger-definitions/greeting-definition.yaml ./services/controllers --overwrite true --isEmptyBody false",
    "test-generate-swagger-default-controller-v2": "nut-swagger generate-default-controller ./swagger-definitions/greeting-definition.yaml ./services/controllers --overwrite true --isEmptyBody false",
    "test-generate-swagger-empty-controllers-from-dir": "node index.js generate-default-controllers-from-dir ./swagger-downstream-definitions ./services/repositories --overwrite true --isEmptyBody true",
    "test-generate-swagger-default-controllers-from-dir": "node index.js generate-default-controllers-from-dir ./swagger-downstream-definitions ./services/repositories --overwrite true --isEmptyBody false"
  },
  "bin": {
    "nut-swagger": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodejs-projects-kenanhancer/nut-swagger.git"
  },
  "keywords": [],
