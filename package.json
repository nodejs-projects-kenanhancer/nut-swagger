  "bugs": {
    "url": "https://github.com/nodejs-projects-kenanhancer/nut-swagger/issues"
  },
  "homepage": "https://github.com/nodejs-projects-kenanhancer/nut-swagger#readme",
  "dependencies": {
    "nut-ioc": "^1.0.4",
    "open": "^7.0.3",
    "yargs": "^15.3.1"
  }
}
